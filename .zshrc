plugins=(git history zsh-autosuggestions zsh-syntax-highlighting sudo copypath copyfile copybuffer dirhistory)

# git aliases
alias gck="git checkout -b"
alias gst="git status"
alias gsh="git show"
alias glog="git log --oneline"
alias gadd="git add"
alias gmt="git commit -m"
alias grb="git rebase -i"

# node aliases
alias ystart="yarn  start"
alias ydev="yarn dev"
alias ytu="yarn test:unit"
